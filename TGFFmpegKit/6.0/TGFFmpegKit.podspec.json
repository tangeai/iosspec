{
  "name": "TGFFmpegKit",
  "version": "6.0",
  "summary": "FFmpeg Kit iOS Full Shared Framework",
  "description": "Includes FFmpeg with dav1d, fontconfig, freetype, fribidi, gmp, gnutls, kvazaar, lame, libass, libilbc, libtheora, libvorbis, libvpx, libwebp, zimg, libxml2, opencore-amr, opus, shine, snappy, soxr, speex, twolame and vo-amrwbenc libraries enabled.",
  "homepage": "https://gitee.com/tange-ai/tgffmpegkit.git",
  "authors": {
    "ARTHENICA": "open-source@arthenica.com"
  },
  "license": {
    "type": "LGPL-3.0",
    "file": "ffmpegkit.xcframework/ios-arm64/ffmpegkit.framework/LICENSE"
  },
  "platforms": {
    "ios": "12.1"
  },
  "requires_arc": true,
  "libraries": [
    "z",
    "bz2",
    "c++",
    "iconv"
  ],
  "source": {
    "http": "https://gitee.com/tange-ai/tgffmpegkit.git"
  },
  "ios": {
    "frameworks": [
      "AudioToolbox",
      "AVFoundation",
      "CoreMedia",
      "VideoToolbox"
    ],
    "vendored_frameworks": [
      "ios/ffmpegkit.xcframework",
      "ios/libavcodec.xcframework",
      "ios/libavdevice.xcframework",
      "ios/libavfilter.xcframework",
      "ios/libavformat.xcframework",
      "ios/libavutil.xcframework",
      "ios/libswresample.xcframework",
      "ios/libswscale.xcframework"
    ]
  }
}
